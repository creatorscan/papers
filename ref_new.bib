@Inproceedings {export:118813,
abstract     = {<p>We describe an acoustic modeling approach in which all phonetic states share a
                common Gaussian Mixture Model structure, and the means and mixture weights vary
                in a subspace of the total parameter space.  We call this a Subspace Gaussian
                Mixture Model (SGMM). Globally shared parameters define the subspace.  This style
                of acoustic model allows for a much more compact representation and gives better
                results than a conventional modeling approach, particularly with smaller amounts
                of training data.</p>},
author       = {Daniel Povey and Lukas Burget and Mohit Agarwal and Pinar Akyazi and Kai Feng and
                Arnab Ghoshal and Ondrej Glembek and Nagendra Kumar Goel and Martin Karafiat and
                Ariya Rastrow and Richard C. Rose and Petr Schwarz and Samuel Thomas},
booktitle    = {International Conference on Acoustics, Speech, and Signal Processing},
title        = {Subspace Gaussian Mixture Models for Speech Recognition},
